<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/AdminLTEcss")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">

            <!-- Logo -->
            <a href="index2.html" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>RPB</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg">Reporte de <b>Bache </b></span>
            </a>

            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <!-- Navbar Right Menu -->
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Messages: style can be found in dropdown.less-->
                       
                        <!-- User Account: style can be found in dropdown.less -->
                        <!-- Control Sidebar Toggle Button -->
                        <li>
                            <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                        </li>
                    </ul>
                </div>

            </nav>
        </header>

        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->

                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="header"><b>Datos Usuario</b></li>
                    <li class="active treeview">
                        <a href="#">
                            <i class="fa fa-book"></i> <span>Usuario</span> <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="index.html"><i class="fa fa-circle-o"></i> Editar</a></li>
                        </ul>
                    </li>
           
                    <li class="treeview">
                        <a href="#">
                            <i class="fa fa-edit"></i> <span>Bache</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li><a href="pages/forms/general.html"><i class="fa fa-circle-o"></i> Insertar </a></li>
                            <li><a href="pages/forms/advanced.html"><i class="fa fa-circle-o"></i> Modificar </a></li>
                            <li><a href="pages/forms/editors.html"><i class="fa fa-circle-o"></i> Eliminar </a></li>
                        </ul>
                    </li>
                  
                    <li class="header"><b>Consultas</b></li>
                    <li><a href="#"><i class="fa fa-folder"></i> <span> Reportes </span></a></li>
                    <li><a href="#"><i class="fa fa-folder"></i></i> <span> Usuarios Regitrados</span></a></li>
                    <li><a href="#"><i class="fa fa-folder"></i></i> <span> Calles Parcheadas</span></a></li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>

        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
            
                <ol class="breadcrumb">
                    <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                    <li class="active">Dashboard</li>
                </ol>
            </section>
            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section>
        </div>

        <footer class="main-footer">
       
            <strong>Copyright &copy; 2020 <a href="http://www.mici.gob.pa">Ingenieria de Software II</a>.</strong> Derechos Reservados
        </footer>
    </div>

    @Scripts.Render("~/Content/AdminLTEJs")
    @RenderSection("scripts", required: false)
</body>
</html>
